        </activity>
        <activity android:name=".Activitys.PedidosActivity">
        </activity>
        <activity android:name=".Activitys.StockActivity">
        </activity>
        <activity android:name=".Activitys.AdministrarStockActivity">
        </activity>
        <activity android:name=".Activitys.ControlarStockActivity">
        </activity>
        <activity android:name=".Activitys.NewPedidoActivity">
        </activity>
        <activity android:name=".Activitys.DatosNewPedidoActivity">
        </activity>
        <activity android:name=".Activitys.DatosPedidoActivity">
        </activity>
        <activity android:name=".Activitys.InfoActivity">
        </activity>
    </application>
</manifest>